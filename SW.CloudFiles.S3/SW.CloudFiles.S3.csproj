<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <PackageId>SimplyWorks.CloudFiles.S3</PackageId>
    <Copyright>Simplify9</Copyright>
    <Company>Simplify9</Company>
    <Authors>Simplify9</Authors>
    <Product>SimplyWorks.CloudFiles.S3</Product>
    <PackageLicenseExpression>MIT</PackageLicenseExpression> 
    <PackageProjectUrl>https://github.com/simplify9/CloudFiles</PackageProjectUrl>
